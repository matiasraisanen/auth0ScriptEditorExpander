{
    "manifest_version": 2,
    "name": "Auth0 code editor expander",
    "version": "1.0.0",
    "description": "Expands that teeny weeny browser Script editor on auth0, so that it's easier to edit scripts in the browser",
    "content_scripts": [
        {
            "css": [
                "./styles.css"
            ],
            "matches": [
                "https://manage.auth0.com/dashboard/*"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "images/icon.png"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    }
}